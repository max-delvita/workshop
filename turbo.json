{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**"],
			"env": [
				"TALLY_API_KEY",
				"TALLY_FORM_ID",
				"TALLY_SYNC_TOKEN",
				"OPENAI_API_KEY",
				"NEXT_PUBLIC_CONVEX_URL"
			]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"dev:setup": {
			"cache": false,
			"persistent": true
		}
	}
}
